<!-- Modal-->
<div class="modal fade" id="addHealthConcernModal" tabindex="-1" role="dialog" aria-labelledby="HealthConcernModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="HealthConcernmyModalLabel">Add New Health Concern</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{ var m = new Grow.Models.HealthConcern();
                    <partial name="_HealthConcernCreate" model="m" />
                }
            </div>
        </div>
    </div>
</div>
